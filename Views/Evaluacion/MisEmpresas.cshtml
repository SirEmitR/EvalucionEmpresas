@{
}
<div class="container">
    <div class="row">
        <h2 class="col-8 mb-4"> @ViewData["id"]</h2>
    </div>
  
    <div class="row col-12 container p-3 border">
        @foreach (var empresas in ViewBag.misEmpresas)
        {
    <div class=" row col-12 mb-3">
        <span class=" col-3 text-center">@empresas.Empresa</span>
        <span class=" col-3 text-center mr-3">@empresas.RazonSocial</span>
        <a href="#" class="btn btn-dark col-1 p-2 disabled ml-3">Evaluar</a>
        <a href="#" class="btn btn-dark col-2 p-2 disabled ml-3">Ver evaluación</a>
        <a href="#" class="btn btn-dark col-2 p-2 disabled ml-3">Descargar evaluación</a>
    </div>
        }
    </div>
</div>